{
  "name": "probaclick",
  "version": "1.1.1",
  "description": "Do something when someone is probably going to click something.",
  "main": "src/probaclick.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmacarthur/probaclick.git"
  },
  "keywords": [
    "hover",
    "intention"
  ],
  "scripts": {
    "test": "jest",
    "prettify": "prettier src/* --write",
    "dev": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js --watch",
    "build": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js && npm run prettify",
    "prettify-and-add": "npm run prettify && git add -A",
    "serve": "concurrently --kill-others \"npm run dev\" \"node serve.js\""
  },
  "author": "Alex MacArthur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexmacarthur/probaclick/issues"
  },
  "pre-commit": [
    "prettify-and-add"
  ],
  "homepage": "https://github.com/alexmacarthur/probaclick#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babili-webpack-plugin": "^0.1.2",
    "banner-webpack-plugin": "^0.2.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "jest-date-mock": "^1.0.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.2",
    "webpack": "^3.8.1"
  }
}
